          <a title="{{name}} {{kind}} Reference"></a>
          {{> header}}
          <div class="content-wrapper">
            <p id="breadcrumbs">
              <a href="{{path_to_root}}index.html">{{module_name}} Reference</a>
              <img id="carat" src="{{path_to_root}}img/carat.png" />
