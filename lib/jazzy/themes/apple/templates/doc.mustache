            </p>
          </div>
          <div class="content-wrapper">
            {{> nav}}
            <article class="main-content">
              <section>
                <section class="section">
                  {{^hide_name}}<h1>{{name}}</h1>{{/hide_name}}
                  {{#declaration}}
                    <div class="declaration">
                      <div class="language">
                        {{{declaration}}}
                      </div>
                    </div>
                  {{/declaration}}
                  {{{overview}}}
                </section>
                {{> tasks}}
              </section>
              {{> footer}}
